{
  "name": "openui5-ratingshroom",
  "version": "1.0.5",
  "description": "An OpenUI5 Control created for SAP Inside Track Sydney 1811",
  "homepage": "https://github.com/mauriciolauffer/openui5-ratingshroom#readme",
  "author": "Mauricio Lauffer",
  "license": "MIT",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5",
    "ui5lab",
    "rating",
    "supermariobros",
    "mariobros",
    "mushroom"
  ],
  "main": "dist/openui5/ratingshroom/library-preload.js",
  "files": [
    "dist",
    "src",
    "ui5.yaml"
  ],
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "eslint": "^8.39.0",
    "eslint-config-mlauffer-ui5": "^0.3.3",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-coveralls": "^2.1.0",
    "karma-ui5": "^3.0.2"
  },
  "scripts": {
    "start": "ui5 serve",
    "start:dist": "ui5 serve --config ui5-dist.yaml",
    "build": "ui5 build --clean-dest",
    "lint": "eslint src test demo",
    "lint:ci": "eslint src test -f @microsoft/eslint-formatter-sarif -o target/eslint.sarif",
    "lint:fix": "eslint src test demo --fix",
    "test": "karma start",
    "test:ci": "karma start karma.ci.conf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriciolauffer/openui5-ratingshroom.git"
  },
  "bugs": {
    "url": "https://github.com/mauriciolauffer/openui5-ratingshroom/issues"
  }
}
